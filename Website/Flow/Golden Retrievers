    <li> Age: </li>
    <li> 
  </ul>
</div>

<script>
function dogTitle() {
    var x = document.getElementById('dogTitle');
    if (x.style.display === 'none') {
      x.style.display = 'block';
  } else {
      x.style.display = 'none';
  }
}
function dogTypes() {
    var x = document.getElementById('dogTypes');
    if (x.style.display === 'none') {
      x.style.display = 'block';
  } else {
      x.style.display = 'none';
  }
}
</script>
